    "lst = []\n",
    "n = int(input(\"Enter number of elements : \"))\n",
    "for i in range(n):\n",
    "    ele = int(input(\"Enter an element:\"))\n",
    "    if ele % 2==1:\n",
    "        lst.append(ele)\n",
    "x=sum(lst)\n",
    "print(\"The sum of odd elements in the list is {}.\".format(x))"
